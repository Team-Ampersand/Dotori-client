{
  "name": "dotori-client",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": ".",
  "productName": "Dotori",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-csv": "^1.1.2",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "5.1.8",
    "@types/react-slick": "^0.23.8",
    "@types/styled-components": "^5.1.11",
    "axios": "^0.21.1",
    "electron-is-dev": "^2.0.0",
    "firebase": "^9.6.9",
    "global": "^4.4.0",
    "jwt-decode": "^3.1.2",
    "polished": "^4.1.3",
    "prettier": "^2.5.1",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-beforeunload": "^2.5.1",
    "react-calendar": "^3.5.0",
    "react-cookie": "^4.1.1",
    "react-csv": "^2.2.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.2",
    "react-scripts": "4.0.3",
    "react-slick": "^0.28.1",
    "react-toastify": "^8.2.0",
    "recoil": "^0.7.0",
    "recoil-persist": "^4.0.0",
    "slick-carousel": "^1.8.1",
    "styled-components": "^5.3.0",
    "swr": "^1.1.2",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "build": {
    "productName": "Dotori",
    "appId": "com.dotori.electron",
    "asar": true,
    "mac": {
      "icon": "./public/icons/mac/icon.icns",
      "target": [
        "default"
      ]
    },
    "dmg": {
      "title": "Dotori"
    },
    "win": {
      "icon": "./public/icons/png/256x256.png",
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": false,
      "createDesktopShortcut": true,
      "perMachine": true,
      "language": 1042,
      "shortcutName": "Dotori",
      "artifactName": "Dotori-${version}.${ext}",
      "uninstallDisplayName": "Dotori"
    },
    "directories": {
      "buildResources": "./build/",
      "output": "./dist/"
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "react-start": "BROWSER=none yarn start",
    "electron-start": "ELECTRON_START_URL=https://dotori-gsm.com/ electron .",
    "electron-mac": "electron-builder --mac",
    "electron-win32": "electron-builder --win --ia32",
    "electron-win64": "electron-builder --win --x64",
    "electron-all": "yarn electron-mac && yarn electron-win32 && yarn electron-win64"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "asar": "^3.1.0",
    "electron": "^18.2.4",
    "electron-builder": "^23.0.3"
  }
}
